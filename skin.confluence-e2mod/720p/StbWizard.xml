	<coordinates>
		<system>1</system>
		<posx>0</posx>
		<posy>0</posy>
	</coordinates>
	<include>dialogeffect</include>
	<controls>
		<control type="image">
			<posx>0</posx>
			<posy>0</posy>
			<width>1280</width>
			<height>720</height>
			<texture>special://skin/backgrounds/SKINDEFAULT.jpg</texture>
		</control>
		<control type="image">
			<posx>50</posx>
			<posy>50</posy>
			<width>1180</width>
			<height>620</height>
			<texture border="15">ContentPanel.png</texture>
		</control>
		<control type="image">
			<posx>65</posx>
			<posy>65</posy>
			<width>1150</width>
			<height>590</height>
			<texture border="40">black-back2.png</texture>
		</control>
