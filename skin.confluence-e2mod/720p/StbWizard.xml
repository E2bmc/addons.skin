			<posx>65</posx>
			<posy>5</posy>
			<width>1000</width>
			<height>30</height>
			<orientation>horizontal</orientation>
			<align>left</align>
			<itemgap>5</itemgap>
			<control type="label" id="2">
				<include>WindowTitleCommons</include>
				<label>$INFO[Window.Property(header_text)]</label>
			</control>
		</control>
		<include>Clock</include>
