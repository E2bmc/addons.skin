			<posx>75</posx>
			<posy>80</posy>
			<width>600</width>
			<height>565</height>
			<viewtype label="535">list</viewtype>
			<pagecontrol>60</pagecontrol>
			<scrolltime>200</scrolltime>
			<itemlayout height="30" width="690">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>30</width>
					<height>30</height>
					<info>ListItem.Icon</info>
				</control>
				<control type="label">
					<posx>30</posx>
					<posy>0</posy>
					<width>550</width>
					<height>30</height>
					<font>font24</font>
					<textcolor>white</textcolor>
					<selectedcolor>selected</selectedcolor>
					<align>left</align>
					<aligny>center</aligny>
					<label>$INFO[ListItem.Label]</label>
				</control>
			</itemlayout>
			<focusedlayout height="30" width="580">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>580</width>
					<height>30</height>
					<texture border="5">button-nofocus.png</texture>
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>580</width>
					<height>30</height>
					<texture border="5">button-focus2.png</texture>
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>30</width>
					<height>30</height>
					<info>ListItem.Icon</info>
				</control>
				<control type="label">
					<posx>30</posx>
					<posy>0</posy>
					<width>550</width>
					<height>30</height>
					<font>font24</font>
					<textcolor>white</textcolor>
					<selectedcolor>selected</selectedcolor>
					<align>left</align>
					<aligny>center</aligny>
					<label>$INFO[ListItem.Label]</label>
				</control>
			</focusedlayout>
		</control>
		<control type="scrollbar" id="60">
			<posx>660</posx>
			<posy>80</posy>
			<width>20</width>
			<height>580</height>
